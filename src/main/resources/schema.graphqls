type Query {
  personByFiscalCode(text: String): [Person]
  getExams(personId: String): [Exam]
}

type Person {
	id: ID
	name: String
	surname: String
	fiscalCode: String
}

type Exam {
	id: ID
	title: String
	description: String
	entityType: String
	dateFrom: String
	dateTo: String
	validityFrom: String
	validityTo: String
	type: String
	qualification: String
	honour: Boolean
	grade: String
	result: Boolean
	externalCandidate: Boolean
}